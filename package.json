{
  "name": "gigabyte-web-site",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "type": "module",
  "lib": "es2021",
  "scripts": {
    "node": "node --es-module-specifier-resolution=node",
    "service:check": "npm run node src/services/checkGigabyteRU.js",
    "service:images": "npm run node src/services/productImages.js",
    "service:triggers": "npm run node src/services/createAllTriggers.js",
    "server": "npm run node src/server.js",
    "start": "npm run node src/init.js",
    "test": "npm run node src/test.js",
    "update": "npm run node src/update.js",
    "pm2:start": "pm2 start npm -- run start",
    "pm2:start-watch": "pm2 start npm -- run start --watch",
    "pm2:delete": "pm2 delete npm -- run start",
    "cluster": "pm2 -i 4 start npm -- run start"
  },
  "author": "Yury Nesterenko",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "mysql2": "^2.3.3",
    "pm2": "^5.1.2",
    "redis": "^4.0.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "2.5.1"
  }
}
